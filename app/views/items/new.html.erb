<h1><%= @list.title %></h1>

  <p>
    <% @list.items.each do |item| %>
      <%= item.title %><br />
      <%= link_to 'Show', list_path(item) %>
      <%= link_to 'Edit', edit_list_path(item) %>
      <%= link_to 'Destroy', list_path(item),
          method: :delete, data: { confirm: 'Are you sure?'}
          %>
</p>

      <% end %>
