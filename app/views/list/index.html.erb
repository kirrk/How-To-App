<h1>My How-To-Lists</h1>

  <p>
    <% @list.each do |l| %>
      <%= l.title %><br />
      <%= link_to 'Show', list_path(l) %>
      <%= link_to 'Edit', edit_list_path(l) %>
      <%= link_to 'Delete', list_path(l),
          method: :delete, data: { confirm: 'Are you sure?'}
          %><br /><br />

</p>
      <% end %>
      <%= link_to 'Create a New List', new_list_path %>
