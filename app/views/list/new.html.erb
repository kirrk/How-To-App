<h1>New List</h1>


  <%= form_for :list, url: list_index_path do |f| %>
    <% if @list.errors.any? %>
    <div id="error_explanation">
        <h2><%= pluralize(@list.errors.count, "error") %> Prohibited
          this list from being saved: </h2>
        <ul>
          <% @list.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
          <% end %>
        </ul>
      </div>
          <% end %>
    <p><%= f.label :title %> <br />
     <%= f.text_field :title %>
    </p>


    <p>
      <%= f.submit %>
    </p>
    <% end %>

    <%= link_to 'Back', list_index_path %>


